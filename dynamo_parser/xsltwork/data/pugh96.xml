<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xmile >
    <sim_specs>
        <stop>50.0</stop>
        <start>0.0</start>
        <dt>0.25</dt>
    </sim_specs>
    <model name="default">
        <variables>
            <stock name="SUSC">
                <outflow>INF</outflow>
                <eqn>988</eqn>
            </stock>
            <flow name="INF">
                <eqn>SICK*CNTCTS*FRSICK</eqn>
            </flow>
            <aux name="FRSICK">
                <eqn>0.05</eqn>
            </aux>
            <stock name="SICK">
                <outflow>CURE</outflow>
                <inflow>INF</inflow>
                <eqn>2</eqn>
            </stock>
            <aux name="TABIN">
                <eqn>SUSC/TOTAL</eqn>
            </aux>
            <aux name="CNTCTS">
                <eqn>TABIN</eqn>
                <gf name="TABCON"/>
            </aux>
            <gf name="TABCON" type="continuous">
                <xscale min="0.0" max="1.0"/>
                <ypts>0,2.8,5.5,8,9.5,10</ypts>
            </gf>
            <aux name="TOTAL">
                <eqn>INIT(SUSC)+INIT(SICK)+INIT(RECOV)</eqn>
            </aux>
            <flow name="CURE">
                <eqn>SICK/DUR</eqn>
            </flow>
            <aux name="DUR">
                <eqn>10</eqn>
            </aux>
            <stock name="RECOV">
                <inflow>CURE</inflow>
                <eqn>10</eqn>
            </stock>
        </variables>
        <views>
            <view>
                <connector uid="0">
                    <from>TABIN</from>
                    <to>CNTCTS</to>
                </connector>
                <connector uid="1" color="#E8E8E8">
                    <from>SUSC</from>
                    <to>TOTAL</to>
                </connector>
                <connector uid="2" color="#E8E8E8">
                    <from>SICK</from>
                    <to>TOTAL</to>
                </connector>
                <connector uid="3" color="#E8E8E8">
                    <from>RECOV</from>
                    <to>TOTAL</to>
                </connector>
                <connector uid="4">
                    <from>SICK</from>
                    <to>INF</to>
                </connector>
                <connector uid="5">
                    <from>CNTCTS</from>
                    <to>INF</to>
                </connector>
                <connector uid="6">
                    <from>FRSICK</from>
                    <to>INF</to>
                </connector>
                <connector uid="7">
                    <from>SUSC</from>
                    <to>TABIN</to>
                </connector>
                <connector uid="8">
                    <from>TOTAL</from>
                    <to>TABIN</to>
                </connector>
                <connector uid="9">
                    <from>SICK</from>
                    <to>CURE</to>
                </connector>
                <connector uid="10">
                    <from>DUR</from>
                    <to>CURE</to>
                </connector>
                <stacked_container uid="11">
                    <graph show_grid="true" plot_numbers="true" background="yellow">
                        <plot index="1">
                            <entity name="SUSC"/>
                        </plot>
                        <plot index="2">
                            <entity name="SICK"/>
                        </plot>
                        <plot index="3">
                            <entity name="RECOV"/>
                        </plot>
                        <plot index="4">
                            <entity name="INF"/>
                        </plot>
                        <plot index="5">
                            <entity name="CURE"/>
                        </plot>
                    </graph>
                </stacked_container>
            </view>
        </views>
    </model>
</xmile>
