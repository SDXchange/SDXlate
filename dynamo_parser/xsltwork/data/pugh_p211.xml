<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xmile >
    <sim_specs>
        <stop>100.0</stop>
        <start>0.0</start>
        <dt>0.25</dt>
    </sim_specs>
    <model name="default">
        <variables>
            <aux name="APPRG">
                <eqn>WF*GPROD</eqn>
            </aux>
            <aux name="GPROD">
                <eqn>1</eqn>
            </aux>
            <flow name="RPRG">
                <eqn>APPRG*FSAT</eqn>
            </flow>
            <aux name="FSAT">
                <eqn>0.7</eqn>
            </aux>
            <stock name="CRPRG">
                <inflow>RPRG</inflow>
                <eqn>0</eqn>
            </stock>
            <flow name="GURW">
                <eqn>APPRG*(1-FSAT)</eqn>
            </flow>
            <stock name="URW">
                <outflow>DURW</outflow>
                <inflow>GURW</inflow>
                <eqn>0</eqn>
            </stock>
            <flow name="DURW">
                <eqn>URW/TDRW</eqn>
            </flow>
            <aux name="TDRW">
                <eqn>FPCOMP</eqn>
                <gf name="TTDRW"/>
            </aux>
            <gf name="TTDRW" type="continuous">
                <xscale min="0.0" max="1.0"/>
                <ypts>12,12,12,10,5,.5</ypts>
            </gf>
            <aux name="CPPRG">
                <eqn>CRPRG+URW</eqn>
            </aux>
            <aux name="FPCOMP">
                <eqn>CPPRG/IPD</eqn>
            </aux>
            <aux name="IPD">
                <eqn>1200</eqn>
            </aux>
            <aux name="EPREM">
                <eqn>(IPD-CPPRG)/PPROD</eqn>
            </aux>
            <aux name="PPROD">
                <eqn>SMTH1(IPROD,TPPROD)</eqn>
            </aux>
            <aux name="TPPROD">
                <eqn>6</eqn>
            </aux>
            <aux name="IPROD">
                <eqn>WTRP*RPROD+(1-WTRP)*GPROD</eqn>
            </aux>
            <aux name="RPROD">
                <eqn>GPROD*FSAT</eqn>
            </aux>
            <aux name="WTRP">
                <eqn>FPCOMP</eqn>
                <gf name="TWTRP"/>
            </aux>
            <gf name="TWTRP" type="continuous">
                <xscale min="0.0" max="1.0"/>
                <ypts>0,.1,.25,.5,.9,1</ypts>
            </gf>
            <stock name="WF">
                <inflow>HR</inflow>
                <eqn>WFN</eqn>
            </stock>
            <aux name="WFN">
                <eqn>2</eqn>
            </aux>
            <flow name="HR">
                <eqn>(WFS-WF)/WFAT</eqn>
            </flow>
            <aux name="WFAT">
                <eqn>3</eqn>
            </aux>
            <aux name="WFS">
                <eqn>WCWF*IWF+(1-WCWF)*WF</eqn>
            </aux>
            <aux name="IWF">
                <eqn>EPREM/TREM</eqn>
            </aux>
            <aux name="WCWF">
                <eqn>TREM</eqn>
                <gf name="TWCWF"/>
            </aux>
            <gf name="TWCWF" type="discrete">
                <xscale min="0.0" max="21.0"/>
                <ypts>0,0,0,.1,.3,.7,.9,1</ypts>
            </gf>
            <aux name="TREM">
                <eqn>SCD-TIME</eqn>
            </aux>
            <stock name="SCD">
                <inflow>NAS</inflow>
                <eqn>SCDN</eqn>
            </stock>
            <aux name="SCDN">
                <eqn>40</eqn>
            </aux>
            <flow name="NAS">
                <eqn>(ICD-SCD)/SAT</eqn>
            </flow>
            <aux name="SAT">
                <eqn>6</eqn>
            </aux>
            <aux name="ICD">
                <eqn>TiME+TPREQ</eqn>
            </aux>
            <aux name="TPREQ">
                <eqn>EPREM/WFS</eqn>
            </aux>
            <aux name="LENGTH">
                <eqn>IF(1&lt;FCOMP)THEN(0)ELSE(MAXLEN)</eqn>
            </aux>
            <aux name="FCOMP">
                <eqn>CRPRG/IPD</eqn>
            </aux>
        </variables>
        <views>
            <view>
                <connector uid="0">
                    <from>WTRP</from>
                    <to>IPROD</to>
                </connector>
                <connector uid="1">
                    <from>RPROD</from>
                    <to>IPROD</to>
                </connector>
                <connector uid="2">
                    <from>GPROD</from>
                    <to>IPROD</to>
                </connector>
                <connector uid="3">
                    <from>APPRG</from>
                    <to>RPRG</to>
                </connector>
                <connector uid="4">
                    <from>FSAT</from>
                    <to>RPRG</to>
                </connector>
                <connector uid="5">
                    <from>FPCOMP</from>
                    <to>WTRP</to>
                </connector>
                <connector uid="6">
                    <from>WFS</from>
                    <to>HR</to>
                </connector>
                <connector uid="7">
                    <from>WF</from>
                    <to>HR</to>
                </connector>
                <connector uid="8">
                    <from>WFAT</from>
                    <to>HR</to>
                </connector>
                <connector uid="9">
                    <from>ICD</from>
                    <to>NAS</to>
                </connector>
                <connector uid="10">
                    <from>SCD</from>
                    <to>NAS</to>
                </connector>
                <connector uid="11">
                    <from>SAT</from>
                    <to>NAS</to>
                </connector>
                <connector uid="12">
                    <from>WCWF</from>
                    <to>WFS</to>
                </connector>
                <connector uid="13">
                    <from>IWF</from>
                    <to>WFS</to>
                </connector>
                <connector uid="14">
                    <from>WF</from>
                    <to>WFS</to>
                </connector>
                <connector uid="15">
                    <from>SCD</from>
                    <to>TREM</to>
                </connector>
                <connector uid="16">
                    <from>TIME</from>
                    <to>TREM</to>
                </connector>
                <connector uid="18">
                    <from>MAXLEN</from>
                    <to>LENGTH</to>
                </connector>
                <connector uid="19">
                    <from>FCOMP</from>
                    <to>LENGTH</to>
                </connector>
                <connector uid="20">
                    <from>FPCOMP</from>
                    <to>TDRW</to>
                </connector>
                <connector uid="21">
                    <from>CRPRG</from>
                    <to>FCOMP</to>
                </connector>
                <connector uid="22">
                    <from>IPD</from>
                    <to>FCOMP</to>
                </connector>
                <connector uid="23">
                    <from>IPD</from>
                    <to>EPREM</to>
                </connector>
                <connector uid="24">
                    <from>CPPRG</from>
                    <to>EPREM</to>
                </connector>
                <connector uid="25">
                    <from>PPROD</from>
                    <to>EPREM</to>
                </connector>
                <connector uid="26">
                    <from>SMTH1(,)</from>
                    <to>PPROD</to>
                </connector>
                <connector uid="27">
                    <from>IPROD</from>
                    <to>PPROD</to>
                </connector>
                <connector uid="28">
                    <from>TPPROD</from>
                    <to>PPROD</to>
                </connector>
                <connector uid="29">
                    <from>GPROD</from>
                    <to>RPROD</to>
                </connector>
                <connector uid="30">
                    <from>FSAT</from>
                    <to>RPROD</to>
                </connector>
                <connector uid="31">
                    <from>TiME</from>
                    <to>ICD</to>
                </connector>
                <connector uid="32">
                    <from>TPREQ</from>
                    <to>ICD</to>
                </connector>
                <connector uid="33">
                    <from>CRPRG</from>
                    <to>CPPRG</to>
                </connector>
                <connector uid="34">
                    <from>URW</from>
                    <to>CPPRG</to>
                </connector>
                <connector uid="35">
                    <from>APPRG</from>
                    <to>GURW</to>
                </connector>
                <connector uid="36">
                    <from>FSAT</from>
                    <to>GURW</to>
                </connector>
                <connector uid="37">
                    <from>EPREM</from>
                    <to>TPREQ</to>
                </connector>
                <connector uid="38">
                    <from>WFS</from>
                    <to>TPREQ</to>
                </connector>
                <connector uid="39">
                    <from>CPPRG</from>
                    <to>FPCOMP</to>
                </connector>
                <connector uid="40">
                    <from>IPD</from>
                    <to>FPCOMP</to>
                </connector>
                <connector uid="41">
                    <from>WF</from>
                    <to>APPRG</to>
                </connector>
                <connector uid="42">
                    <from>GPROD</from>
                    <to>APPRG</to>
                </connector>
                <connector uid="43">
                    <from>TREM</from>
                    <to>WCWF</to>
                </connector>
                <connector uid="44">
                    <from>EPREM</from>
                    <to>IWF</to>
                </connector>
                <connector uid="45">
                    <from>TREM</from>
                    <to>IWF</to>
                </connector>
                <connector uid="46">
                    <from>URW</from>
                    <to>DURW</to>
                </connector>
                <connector uid="47">
                    <from>TDRW</from>
                    <to>DURW</to>
                </connector>
                <stacked_container uid="48">
                    <graph show_grid="true" plot_numbers="true" background="yellow">
                        <plot index="1">
                            <entity name="WF"/>
                        </plot>
                        <plot index="2">
                            <entity name="SCD"/>
                        </plot>
                        <plot index="3">
                            <entity name="CPPRG"/>
                        </plot>
                        <plot index="4">
                            <entity name="CRPRG"/>
                        </plot>
                        <plot index="5">
                            <entity name="URW"/>
                        </plot>
                        <plot index="6">
                            <entity name="PPROD"/>
                        </plot>
                    </graph>
                </stacked_container>
            </view>
        </views>
    </model>
</xmile>

